{
    "version": 20,
    "variables": [
        {
            "version": 2,
            "key": "Perfil",
            "type": "query",
            "visible": true,
            "editable": true,
            "input": "fetch dt.davis.problems\n| filter isNotNull(labels.alerting_profile) \n| fields  labels.alerting_profile \n",
            "multiple": true,
            "defaultValue": [
                "xxxx"
            ]
        },
        {
            "version": 2,
            "key": "Problema1",
            "type": "query",
            "visible": true,
            "editable": true,
            "input": "fetch dt.davis.problems\n| filter isNotNull(display_id) \n| fields display_id\n| sort display_id asc\n\n\n",
            "multiple": true
        },
        {
            "key": "inicio",
            "visible": true,
            "type": "query",
            "version": 2,
            "editable": true,
            "input": "fetch dt.davis.problems,  from:\"2026-01-01T00:00:00\", to:\"2026-01-25T23:59:59\"\n| filter not(isNull(event.start))\n| fields formatTimestamp(event.start, format:\"yyyy-MM-dd \")\n| sort `formatTimestamp(event.start, format:\"yyyy-MM-dd \")` desc",
            "multiple": false
        },
        {
            "key": "fim",
            "visible": true,
            "type": "query",
            "version": 2,
            "editable": true,
            "input": "fetch dt.davis.problems,  from:\"2026-01-01T00:00:00\", to:\"2026-01-25T23:59:59\"\n| filter not(isNull(event.end))\n| fields formatTimestamp(event.end, format:\"yyyy-MM-dd \")\n\n| sort `formatTimestamp(event.end, format:\"yyyy-MM-dd \")` desc\n\n\n",
            "multiple": false
        },
        {
            "version": 2,
            "key": "Problema2",
            "type": "query",
            "visible": true,
            "editable": true,
            "input": "fetch dt.davis.problems,  from:$inicio, to:$fim\n| filter contains(toString(labels.alerting_profile), $Perfil)\n| fields display_id\n| sort display_id asc",
            "multiple": true
        }
    ],
